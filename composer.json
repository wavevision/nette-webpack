{
  "name": "wavevision/nette-webpack",
  "description": "Webpack adapter for Nette framework.",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Wavevision s.r.o",
      "email": "info@wavevision.com"
    },
    {
      "name": "Vít Rozsíval",
      "email": "vit@wavevision.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Wavevision\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Wavevision\\": "tests",
      "Wavevision\\NetteWebpackExamples\\": "examples/NetteWebpackExamples"
    }
  },
  "config": {
    "platform": {
      "php": "7.4"
    },
    "sort-packages": true
  },
  "require": {
    "php": ">=7.4",
    "latte/latte": "^2.6",
    "nette/di": "^3.0",
    "nette/http": "^3.0",
    "tracy/tracy": "^2.7",
    "wavevision/props-control": "^6.0",
    "wavevision/utils": "^2.4"
  },
  "require-dev": {
    "nette/application": "^3.0",
    "php-coveralls/php-coveralls": "^2.2",
    "phpstan/extension-installer": "^1.0",
    "phpstan/phpstan": "^0.12.11",
    "phpstan/phpstan-nette": "^0.12.3",
    "wavevision/coding-standard": "^5.2",
    "wavevision/di-service-annotation": "^4.0",
    "wavevision/nette-tests": "^3.1"
  },
  "extra": {
    "phpstan": {}
  }
}
